<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Suppliers</title>
  </head>
  <body>
    <h1>List Suppliers</h1>
    <a href="/">return to home</a><br><br>
    <a href="/items/add"><button type="button" name="button">Add Items</button></a>
    <table>
      <thead>
        <th>Id</th>
        <th>Name</th>
        <th>Brand</th>
        <th>Code Items</th>
        <th>List Suppliers</th>
        <th>Add Items</th>
        <th>Actions</th>
      </thead>
      <tbody>
        <%if (data){data.forEach(rows=>{%>
        <tr>
            <td><%=rows.id%></td>
            <td><%=rows.name%></td>
            <td><%=rows.brand%></td>
            <td><%=rows.codeitem%></td>
            <td>list Supplier</td>
            <td>
              <a href="/items/edit/<%=rows.id%>">Edit</a>
              <a href="">|</a>
              <a onclick="javascript:return confirm('Anda yakin akan menghapus data ini?')" href="/items/delete/<%=rows.id%>">Delete</a>
            </td>
        </tr>
        <%})}%>
      </tbody>
    </table>
  </body>
</html>
